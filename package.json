{
  "name": "gestion-pedidos-universal",
  "version": "1.0.0",
  "description": "Sistema completo de gestión de pedidos universal para cualquier empresa",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run backend:dev\" \"npm run frontend:dev\"",
    "backend:dev": "cd backend && npm run dev",
    "frontend:dev": "cd frontend && npm start",
    "migrate": "cd database && node migrate.js",
    "build": "cd frontend && npm run build",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "setup": "node scripts/setup.js"
  },
  "keywords": [
    "pedidos",
    "gestion",
    "universal",
    "empresa"
  ],
  "author": "Sistema de Gestión",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^7.6.0"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "dotenv": "^17.2.1",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.2.1",
    "leaflet": "^1.9.4",
    "lodash": "^4.17.21",
    "mysql2": "^3.14.3",
    "node-fetch": "^2.7.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-leaflet": "^4.2.1",
    "react-toastify": "^11.0.5"
  }
}
